{
  "version": 2,
  "name": "exam-ai-frontend",
  "builds": [
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "outputDirectory": "public",
  "routes": [
    {
      "src": "/",
      "dest": "/index.html"
    },
    {
      "src": "/auth",
      "dest": "/auth.html"
    },
    {
      "src": "/dashboard",
      "dest": "/dashboard.html"
    },
    {
      "src": "/onboarding",
      "dest": "/onboarding.html"
    },
    {
      "src": "/subjects",
      "dest": "/subjects.html"
    },
    {
      "src": "/analysis",
      "dest": "/analysis.html"
    },
    {
      "src": "/results",
      "dest": "/results.html"
    },
    {
      "src": "/topics",
      "dest": "/topics.html"
    },
    {
      "src": "/settings",
      "dest": "/settings.html"
    },
    {
      "src": "/heatmap",
      "dest": "/heatmap.html"
    },
    {
      "src": "/visual-analyzer",
      "dest": "/visual-analyzer.html"
    },
    {
      "src": "/question-predictor",
      "dest": "/question-predictor.html"
    },
    {
      "src": "/question-types",
      "dest": "/question-types.html"
    },
    {
      "src": "/topic-analysis",
      "dest": "/topic-analysis.html"
    },
    {
      "src": "/short-notes",
      "dest": "/short-notes.html"
    },
    {
      "src": "/easy-ways-to-learn",
      "dest": "/easy-ways-to-learn.html"
    },
    {
      "src": "/example-questions",
      "dest": "/example-questions.html"
    },
    {
      "src": "/advanced-question",
      "dest": "/advanced-question.html"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    },
    {
      "source": "/(.*\\.(js|css))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "functions": {},
  "env": {
    "API_BASE_URL": "https://exam-ai-backend.onrender.com"
  }
} 